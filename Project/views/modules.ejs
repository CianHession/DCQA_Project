<style>
    table,th,td{
        border: 1px solid black;
    }
</style>

<table>
    <tr>
    <th>Module ID</th>
    <th>Name</th>
    <th>Credits</th>
    <th>Update</th>
    <th>Students</th>
    </tr>
    <% modules.forEach((module) =>
    { %>
    <tr>
    <td><%= module.mid %></td>
    <td><%= module.name %></td>
    <td><%= module.credits %></td>
    <td><a href="/module/edit/" + module.mid>Update</a></td>
    <td><a href="/">List</a></td>
    </tr>
    <% }) %>
    </table>
    <a href="/">Home</a>
    