<style>
    table,th,td{
        border: 1px solid black;
    }
</style>

<button></a>>Add Student</button>
<br></br>
<table>
    <tr>
    <th>Student ID</th>
    <th>Name</th>
    <th>GPA</th>
    <th>Action</th>
    </tr>
    <% students.forEach((student) =>
    { %>
    <tr>
    <td><%= student.sid %></td>
    <td><%= student.name %></td>
    <td><%= student.gpa %></td>
    <td><a href="/">Delete</a></td>
    </tr>
    <% }) %>
    </table>
    <a href="/">Home</a>
    